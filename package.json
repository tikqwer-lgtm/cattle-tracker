{
  "name": "cattle-tracker",
  "version": "1.4.4",
  "description": "Учёт коров — веб, десктоп, мобильные",
  "scripts": {
    "build": "vite build",
    "test": "vitest run tests/",
    "test:watch": "vitest",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:electron": "node e2e/run-electron-tests.js",
    "e2e:electron:ui": "node e2e/run-electron-tests.js --ui",
    "e2e:electron:log": "node e2e/run-electron-tests-with-log.js",
    "start": "echo Open index.html in browser or run server",
    "start:web": "npx serve -p 9323 .",
    "server": "cd server && npm start",
    "electron": "cd electron && npm start",
    "electron:dist": "cd electron && npm run dist",
    "cap:sync": "cap sync",
    "cap:android": "cap run android",
    "cap:ios": "cap run ios"
  },
  "devDependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@eslint/js": "^10.0.1",
    "@playwright/test": "^1.49.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "serve": "^14.2.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "capacitor": {
    "appId": "com.cattletracker.app",
    "appName": "Учёт коров",
    "webDir": "dist"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  }
}
